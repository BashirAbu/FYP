    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppvsdbg",
            "request": "launch",
            "name": "Debug Game",
            "program": "${workspaceFolder}/../godot/bin/godot.windows.editor.dev.x86_64.exe",
            "args": ["--path", "project/"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "cppvsdbg",
            "request": "launch",
            "name": "Debug Editor",
            "program": "${workspaceFolder}/../godot/bin/godot.windows.editor.dev.x86_64.exe",
            "args": ["-path", "project/project.godot"],
            "cwd": "${workspaceFolder}"
        }
    ]
}